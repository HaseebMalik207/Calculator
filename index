#include <iostream>
using namespce std;
int main(){
	cout << "The sum of two numbers: " << a + b << endl;
	cout << "The difference of two numbers: " << a - b << endl;
	cout << "The product of two numbers: " << a * b << endl;
	cout << "The division of two numbers: " << a / b << endl;
	return 0;
}
